def get_min_max(the_list):

    # define variables
    min = the_list[0]
    max = the_list[0]

    # Find max and min of list
    # sort list
    for x in the_list:
        if x <= min:
            min = x 
        elif x >= max:
            max = x
   
    print(f"This is the minimum: {min}. This is the maximum: {max}.")


# Create list
my_list = [12, 33, 41, 2, 61, 32, 75, 43, 67]
get_min_max(my_list)
